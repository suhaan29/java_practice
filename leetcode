import java.util.ArrayList;
public class NewOne {
	
	public static void main(String[] args)
	{
		ArrayList<Integer> nums = new ArrayList<Integer>();
		nums.add(0);
		nums.add(1);
		nums.add(0);
		nums.add(3);
		nums.add(12);
		System.out.println(nums);
		
		for(int i = 0; i < nums.size(); i++)
		{
		
			if(nums.get(i) == 0)
			{
				nums.remove(i);
				nums.add(0);
			}
		}
		
		System.out.println(nums);
		
	}

}
